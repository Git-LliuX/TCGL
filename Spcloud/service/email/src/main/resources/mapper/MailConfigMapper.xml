<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cosmo.elemes.email.dao.MailConfigMapper">
    <select id="sendMailConfig" resultType="com.cosmo.elemes.email.model.MailConfig">
        select *
        from SY_EMAIL_CONFIG
        where ENABLE_STATUS = 1
          and rownum = 1
    </select>
</mapper>