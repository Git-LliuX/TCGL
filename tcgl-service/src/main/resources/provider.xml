<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <dubbo:application name="${spring.dubbo.application.name}"/>
    <dubbo:protocol name="${spring.dubbo.protocal.name}" port="${spring.dubbo.protocal.port}"/>
    <dubbo:registry address="${spring.dubbo.registry.address}"/>
    <dubbo:annotation package="com.record.tcgl.service.impl"/>

    <dubbo:service interface="com.record.tcgl.api.UserRoleApi" ref="userRoleApi" timeout="100000" />
    <dubbo:service interface="com.record.tcgl.api.AccessRecordApi" ref="accessRecordApi" timeout="100000" />
    <dubbo:service interface="com.record.tcgl.api.PaymentRecordApi" ref="paymentRecordApi" timeout="100000" />
    <dubbo:service interface="com.record.tcgl.api.VehicleOwnerApi" ref="vehicleOwnerApi" timeout="100000" />

</beans>